{
  "name": "@llm-orchestration/core",
  "version": "1.0.1",
  "description": "Generalized LLM Orchestration Framework - A modular, configuration-driven framework for building multi-step AI workflows",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prebuild": "pnpm run lint && pnpm run format:check",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts,.tsx,.mts --config ../eslint.config.mts",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\" --config ../prettier.config.cjs",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\" --config ../prettier.config.cjs",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "llm",
    "orchestration",
    "workflow",
    "ai",
    "typescript",
    "framework",
    "vertex-ai",
    "openai",
    "supabase"
  ],
  "author": "CyberBrown",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.3.0"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "optionalDependencies": {
    "@google-cloud/vertexai": "^1.0.0",
    "@supabase/supabase-js": "^2.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
